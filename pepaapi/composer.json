    {
        "name": "cdsa/pepa_backend",
        "type": "project",
        "description": "PEPA Api.",
        "keywords": [
            "PEPA",
            "laravel"
        ],
        "license": "MIT",
        "require": {
            "php": "^8.1",
            "amphp/amp": ">2.5",
            "amphp/parallel": "^2.3",
            "amphp/socket": "^2.3",
            "amphp/websocket-client": "^2.0",
            "calcinai/phpi": "^0.3.3",
            "cboden/ratchet": "^0.4.1",
            "dompdf/dompdf": "^0.8.0",
            "guzzlehttp/guzzle": "~7.0",
            "hisamu/php-xbase": "^1.2",
            "laravel/framework": "^8.0",
            "laravel/tinker": ">1.0",
            "piphp/gpio": "^0.3.3",
            "ratchet/pawl": "^0.3.1",
            "revolt/event-loop": "^1.0",
            "swiftmailer/swiftmailer": "^6.1",
            "symfony/expression-language": "^4.3",
            "tymon/jwt-auth": "^1.0@dev"
        },
        "require-dev": {
            "kkomelin/laravel-translatable-string-exporter": "^1.23",
            "mockery/mockery": "^1.0",
            "phpunit/phpunit": "^8.0"
        },
        "config": {
            "optimize-autoloader": true,
            "preferred-install": "dist",
            "sort-packages": true
        },
        "extra": {
            "laravel": {
                "dont-discover": []
            }
        },
        "autoload": {
            "psr-4": {
                "App\\": "app/"
            },
            "classmap": [
                "database/seeds",
                "database/factories"
            ]
        },
        "autoload-dev": {
            "psr-4": {
                "Tests\\": "tests/"
            }
        },
        "minimum-stability": "dev",
        "prefer-stable": true,
        "scripts": {
            "post-autoload-dump": [
                "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
                "@php artisan package:discover --ansi"
            ],
            "post-root-package-install": [
                "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
            ],
            "post-create-project-cmd": [
                "@php artisan key:generate --ansi"
            ]
        }
    }
